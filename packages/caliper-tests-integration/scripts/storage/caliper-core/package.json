					".yml",
					".md",
					".html",
					".php"
				],
				"insert_license": false,
				"license_formats": {
					"js": {
						"prepend": "/*",
						"append": "*/",
						"eachLine": {
							"prepend": "* "
						}
					},
					"editorconfig|yaml": {
						"prepend": "#",
						"append": "#",
						"eachLine": {
							"prepend": "# "
						}
					}
				}
			},
			"optionalDependencies": {
				"ajv": "^6.0.0"
			},
			"nyc": {
				"exclude": [
